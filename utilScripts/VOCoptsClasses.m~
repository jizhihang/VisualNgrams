numcls = numel(classes_withPOS);
[classes, classes2, posTags, objname_syns] = deal(cell(numcls,1));
ngramtypes = zeros(numcls,1);
for f=1:numcls
    classes{f}=classes_withPOS{f}{1};
    classes2{f}=classes_withPOS{f}{2};
    posTags{f}=classes_withPOS{f}{3};
    objname_syns{f}=classes_withPOS{f}{4}; 
    ngramtypes(f)=classes_withPOS{f}{5}; 
end
